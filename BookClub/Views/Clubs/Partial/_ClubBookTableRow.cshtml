
@model DAL.Models.ClubBook

<tr>
    <td>
        <a class="display-8" asp-controller="" asp-action="" asp-route-id="@Model.Book.ID">@Model.Book.Name</a>
    </td>
    <td>
        <p class="display-8 mb-0">@Model.Book.Author</p>
    </td>
    @if (Model.ReadUntil.HasValue)
    {
    <td>
        <p class="display-8 mb-0">@Model.ReadUntil.Value</p>
    </td>
    }
</tr>