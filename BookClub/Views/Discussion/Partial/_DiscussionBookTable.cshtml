@model IEnumerable<DAL.Models.ClubDiscussionBook>

<table class="text-center w-100">
    <tr>
        <th>
            <p class="display-7 v2"><strong>Book</strong></p>
        </th>
        <th>
            <p class="display-7 v2"><strong>Author</strong></p>
        </th>
        <th>
            <p class="display-7 v2"><strong>Read by</strong></p>
        </th>
        <th>
            <p class="display-7 v2"><strong>Priority</strong></p>
        </th>
    </tr>
</table>
<div class="w-100" style="max-height: 300px; overflow:auto;">
    <table class="text-center w-100">
        @foreach (var bk in Model)
        {
            await Html.RenderPartialAsync("Partial/_DiscussionBookTableRow", bk);
        }
    </table>
</div>